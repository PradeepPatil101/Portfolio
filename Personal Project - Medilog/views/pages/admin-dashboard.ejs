<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
    <%-include('../partials/bootstrap')%>
        <!-- For datatables from datatables.net -->
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.css">

</head>

<body>
    <%-include('../partials/navigation')%>

        <div class="container">
            <div class="row">

                <div class="col-md-12">
                    <br>
                    <h1 class="display-4" style="text-align: center;">Admin Dashboard</h1>
                    <hr>


                    <div id="carouselExampleIndicators" class="carousel slide carousel-fade" data-interval="false">
                        <ol class="carousel-indicators">
                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                        </ol>
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <div class="row" style="height: 450px; ">
                                    <div class="col-md-12" style="background-color: rgb(41, 41, 41); border-top:5px solid rgb(41, 41, 41); border-bottom:5px solid rgb(88, 88, 88);">
                                        <br>
                                        <br>
                                        <br>
                                        <br>
                                        <br>
                                        <%-include('../partials/dashboard-cards')%>
                                    </div>
                                </div>
                            </div>
                            <div class="carousel-item">
                                <div class="row" style="min-height: 450px;">
                                    <div class="col-md-12" style="background-color: rgb(209, 209, 209); border-top:5px solid rgb(209, 209, 209); border-bottom:5px solid rgb(88, 88, 88);">

                                        <%-include('../partials/dashboard-table1')%>
                                    </div>
                                </div>
                                <br>

                            </div>
                            <div class="carousel-item">
                                <div class="row" style="min-height: 450px;">
                                    <div class="col-md-12" style="background-color: rgb(209, 209, 209); border-top:5px solid rgb(209, 209, 209); border-bottom:5px solid rgb(88, 88, 88);">

                                        <%-include('../partials/dashboard-table2')%>
                                    </div>
                                </div>
                                <br>

                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>







                    </div>





                </div>
            </div>






            <!-- For Carousel -->
            <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

            <!-- For datatables from datatables.net -->
            <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
            <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.js"></script>
            <%-include('../partials/bootstrap-script')%>
                <script type="text/javascript">
                    $(document).ready(function() {
                        $('#receipt_table').DataTable();
                        $('#report_table').DataTable();
                    });
                </script>
</body>



</html>